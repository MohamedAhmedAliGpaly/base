page{height: 100%; background-color: #F2F2F2; font-family:Microsoft YaHei,SimHei,helvetica,arial,verdana,tahoma,sans-serif; font-size:14px;}
h1,h2,h3{font-weight: bold; overflow: hidden; width: 100%; display: block;}
ul{display: block;}
b{font-weight: bold;}
input{text-align: left;}

/* 公共 */
.html{height: 100%;}
.Link{color: #6FB737;}
.loading{line-height: 100px; color: #999; background-color: #F2F2F2; text-align: center;}
.redNum{position: absolute; padding: 0 5px; margin-top: -5px; line-height: 16px; color: #FFF; font-size: 12px; font-weight: normal; border-radius: 10px; background-color: #FF6600;}
.bgImg{background-size: cover; background-position: center; background-repeat: no-repeat;}
.bgTu{width: 100%; height: 100%; border-radius: 50%;}
.bgLogo{background-image: url(https://api.cainiaochefu.com/assets/icon/camera.svg); background-size: 60%;}
.bgImage{background-image: url(https://api.cainiaochefu.com/assets/icon/img.svg); background-size: 60%;}
.divCenter{position: absolute; left: 0; right: 0; top: 0; bottom: 0; margin: auto;}
.ctCenter{position: absolute; margin: 0 auto; left: 0; right: 0;}
.null{text-align: center; color: #999; line-height: 100px;}
.null::before{content: '暂无数据！';}
.noDisplay{display: none;}

/* 箭头 */
.arrow_left{width: 0px; height: 0px; border: 10px solid; border-color:transparent #FFF transparent transparent;}
.arrow_right{width: 0px; height: 0px; border: 10px solid; border-color:transparent transparent transparent #FFF;}
.arrow_up{width: 0px; height: 0px; border: 10px solid; border-color:transparent transparent #FFF transparent;}
.arrow_down{width: 0px; height: 0px; border: 10px solid; border-color:#FFF transparent transparent transparent;}

/* 布局 */
.flex{display: flex; justify-content: space-between; flex-wrap: wrap;}
.flex.center{align-items: center;}
.flex_left{display: flex; justify-content: flex-start; flex-wrap: wrap;}
.flex_right{display: flex; justify-content: flex-end; flex-wrap: wrap;}
.flex_center{display: flex; align-items: center; justify-content: center; height: 100%;}
.flex_nowrap{display: flex; justify-content: space-around; flex-direction: row; white-space:nowrap;}
.flex_in{display: flex; justify-content: center; align-items:center;}
.nowrap{overflow: hidden; white-space: nowrap; text-overflow: ellipsis;}
.nowrap_text{overflow: hidden; text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical;}

/* 底部导航 */
.nav_tabbar text{font-size: 12px;}
.nav_tabbar .van-tabbar-item__icon{margin-bottom: 2px;}
.nav_body{height: calc(100% - 50px); overflow: scroll;}

/* 搜索 */
.Search{margin-left: 10px;}
.Search .input{height: 40px; line-height: 40px; font-size: 14px; padding: 0 10px; border-radius: 20px; background-color: #F2F2F2; color: #999;}
.Search .input text{padding-left: 10px;}

/* 客服 */
.service{position: fixed; z-index: 99; right: 10px; bottom: 60px; width: 48px; height: 48px; line-height: 80px; background-color: #F2F4F6; border-radius: 50%; text-align: center; box-shadow: 0 0 8px rgba(0,0,0,0.3);}
.service van-icon{position: absolute; margin: auto; top: -7px; left: 0; right: 0; bottom: 0;}

/*
* ICO-字体图标
* 图标生成: https://icomoon.io/app/#/select
* 转Base64: https://transfonter.org/
*/
@font-face {
  font-family: 'icomoon';
  src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
      url(data:application/font-woff;charset=utf-8;base64,) format('woff');
  font-weight: normal;
  font-style: normal;
}
.icons{font-family: 'icomoon' !important; font-size: 16px; color: #666;}
.icon_logo:before{content: "\e000";}
.icon_left:before{content: "\e100"; font-size: 20px;}
.icon_right:before{content: "\e101";}
.icon_qrcode:before{content: "\e904"; font-size: 20px;}
.icon_scan:before{content: "\e905"; font-size: 20px;}

.icon_home:before{content: "\e900"; font-size: 16px;}
.icon_shop:before{content: "\e901"; font-size: 16px;}
.icon_cart:before{content: "\e902"; font-size: 20px;}
.icon_me:before{content: "\e903"; font-size: 18px;}

/* UI */
page{font-size: 14px;}
page .van-grid-item__content::after,body .van-hairline--top::after{border-color: #FFF;}
page .van-overlay{z-index: 1000;}
page .van-popup{overflow-y: initial;}
page .van-popup--bottom.van-popup--round{border-radius: 10px 10px 0 0; max-height: calc(100% - 80px);}
page .van-tab--active{color: #6FB737;}
page .van-tabs__line{background-color: #6FB737;}
page .navTitle {text-align: left; margin-left: 45px; color: #333;}
page .navRight {margin-right: 10px; color: #333;}
page .navLeft{position: fixed; color: #333;}